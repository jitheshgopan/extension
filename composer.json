{
	"name": "orchestra/extension",
	"description": "Extension Component for Orchestra Platform",
	"homepage": "http://orchestraplatform.com/docs/latest/components/extension/",
	"keywords": ["orchestra-platform", "orchestral", "laravel", "extension"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\Extension\\" : "src/Extension/"
		}
	},
	"require": {
		"php": ">=5.4.0",
		"orchestra/memory": "2.2.*",
		"orchestra/publisher": "2.2.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"illuminate/config": "4.2.*",
		"illuminate/console": "4.2.*",
		"illuminate/container": "4.2.*",
		"illuminate/events": "4.2.*",
		"illuminate/http": "4.2.*",
		"illuminate/support": "4.2.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/extension component outside of Orchestra Platform",
		"orchestra/foundation": "Allow using orchestra/extension with Orchestra Platform"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.1-dev"
		}
	},
	"minimum-stability": "dev"
}
